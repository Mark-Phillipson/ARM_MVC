@if (menuItems != null)
{
    @foreach (var menuItem in menuItems)
    {
        <div class="nav-item px-3">
            <NavLink class="nav-link" href="@menuItem.PageReference">
                <span class="oi oi-@menuItem.Icon" aria-hidden="true"></span> @menuItem.MenuText
            </NavLink>
        </div>
    }

}

